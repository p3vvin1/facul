num1 = float(input("Digite um numero: "))

if   num1%2==1: 
     print(num1,"  é impar")

else:
     print(num1," não é impar")

if num1%3==0:
    print(num1,"  é multiplo  por 3")

else: 
  print(num1, "não é multiplo de 3")

if   num1%102==0:
     print(num1," é Divisivel por 102 ")

else:
   print(num1," não é divisor por 102")
